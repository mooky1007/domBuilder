{
    "name": "dom_craft",
    "version": "1.1.9",
    "description": "A lightweight, declarative DOM builder for vanilla JavaScript.",
    "main": "dist/dom_craft.umd.min.js",
    "module": "dist/dom_craft.esm.js",
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm run clean && rollup -c",
        "version:patch": "npm version patch && npm run build && npm publish --access public",
        "deploy": "git add . && git commit -m 'release' && git push && npm run version:patch"
    },
    "keywords": [
        "dom",
        "builder",
        "vanilla-js",
        "lightweight",
        "declarative",
        "ui",
        "animation",
        "framework"
    ],
    "author": "mooky1007",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mooky1007/domCraft.git"
    },
    "bugs": {
        "url": "https://github.com/mooky1007/domCraft/issues"
    },
    "homepage": "https://github.com/mooky1007/domCraft#readme",
    "files": [
        "dist",
        "README.md"
    ],
    "devDependencies": {
        "rimraf": "^6.0.1",
        "rollup": "^2.79.2",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-dts": "^5.3.0"
    }
}
